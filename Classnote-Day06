//String methods
//Example 01
//project Sample
package org.test;
import java.lang.String;
public class Sample {
    public static void main(String[] args) {
        //import java.lang.String
        String a = "welcome";
            int length = a.length();
                System.out.println("Length method is "+length);
            char charAt = a.charAt(5);
            	System.out.println("charAt method is "+charAt);
            int indexOf = a.indexOf('e');	
            	System.out.println("indexOf method is "+indexOf);
            int lastIndexOf = a.lastIndexOf('e');
                System.out.println("lastIndexOf method is "+lastIndexOf);
            boolean startsWith = a.startsWith("wel");
                System.out.println("startswith method is "+startsWith);
            boolean endsWith = a.endsWith("ome");
                System.out.println("enddWith method is "+endsWith);
            String toUpperCase = a.toUpperCase();
                System.out.println("toUpperCase method is "+toUpperCase);
        String b = "WELCOME";
            String toLowerCase = b.toLowerCase();
                System.out.println("toLowerCase method is "+toLowerCase);
            boolean equals = a.equals(b);
                System.out.println("equals method is "+equals);
            boolean equalsIgnoreCase = a.equalsIgnoreCase(b);
                System.out.println("equalsIgnoreCase method is "+equalsIgnoreCase);
            String replace = a.replace('e','a');
                System.out.println("replace method is "+replace);
            String replaceAll = a.replaceAll("come","done");
                System.out.println("replaceAll method is "+replaceAll);
            String substring = a.substring(2);
                System.out.println("substring method is "+substring);
            String substring2 = a.substring(1,6);
                System.out.println("subString method is "+substring2);
            String c = "           Hello           ";
                System.out.println(c);    
            String trim = c.trim();
                System.out.println("trim method is "+trim);
            boolean contains = a.contains("elco");
                System.out.println("contains method is "+contains);
            boolean empty = a.isEmpty();
                System.out.println("isEmpty method is "+empty);    
            int compareTo = a.compareTo(b);
                System.out.println("compareTo method is "+compareTo);    
    }
}
/*
Run output is
Length method is 7
charAt method is m
indexOf method is 1
lastIndexOf method is 6
startswith method is true
enddWith method is true
toUpperCase method is WELCOME
toLowerCase method is welcome
equals method is false
equalsIgnoreCase method is true
replace method is walcoma
replaceAll method is weldone
substring method is lcome
subString method is elcom
           Hello           
trim method is Hello
contains method is true
isEmpty method is false
compareTo method si 32
*/





//String Type Literal String immutable and Non Literal String immutalble
//Example 02
//project Sample
package org.test;
import java.util.Hashtable;
public class Sample {
    public static void main(String[] args){
        System.out.println("****Literal String****");
        String s = "Java";
        String s1 = "Java";
            System.out.println("s is "+s);
            System.out.println("S1 is "+s1);
            System.out.println(System.identityHashcode("s is "+s));
            System.out.println(System.identityHashcode("s1 is "+s1));      
        System.out.println("****Non Literal String****");
        String s2 = new String("python");
        String s3 = new String("python");
            System.out.println("s2 is "+s2);
            System.out.println("s3 is "+s3);
            System.out.println(System.identityHashcode(s2));//identityHashcode should be in ITALIC FONT
            System.out.println(System.identityHashcode(s3));
            
    }
}
/* Run time output
****Literal String****
s is Java
S1 is Java
1067040082
1325547227
****Non Literal String****
s2 is python
s3 is python
980546781
2061475679

*/





//String type (Literal change value & Non Literal Mutable change value)
//Example 03
//project Sample
package org.test;
import java.util.HashTable;
public class SampleOne {
    public static void main(String[] args) {
        System.out.println("***Literal-Immutable String");
            String s = "Java";
            String s1 = "Java";
                System.out.println("s is "+s);
                System.out.println("s1 is "+s1);
                System.out.println(System.identityHashcode(s));
                System.out.println(System.identityHashcode(s1));
            s = s.concat(s1);
                System.out.println("s is "+s);
                System.out.println(System.identityHashcode(s));
        System.out.println("***Non Literal-Mutable String***");        
            StringBuffer s2 = new StringBuffer("python");
            StringBuffer s3 = new StringBuffer("python");
                System.out.println(s2);
                System.out.println(s3);
                System.out.println(System.identityHashcode(s2));
                System.out.println(System.identityHashcode(s3));    
            s2 = s2.append(" is language");
            s3 = s3.append(" is language"); 
                System.out.println(s2);
                System.out.println(s3);
                System.out.println(System.indentityHasecode(s2));
                System.out.println(System.indentityHasecode(s3));

                      
    }
}
/* Run Time output
***Literal-Immutable String
s is Java
s1 is Java
1067040082
1067040082
s is JavaJava
1325547227
***Non Literal-Mutable String***
python
python
980546781
2061475679
python is language
python is language
980546781
2061475679
*/







//String Type Literal String and Non Literal immutable String both value change
//Example 04
//project Sample
package org.test;
import java.util.Hashtable;
public class Sample {
    public static void main(String[] args){
        System.out.println("****Literal String****");
        String s = "Java";
        String s1 = "Java";
            System.out.println("s is "+s);
            System.out.println("S1 is "+s1);
            System.out.println(System.identityHashCode(s));
            System.out.println(System.identityHashCode(s1));    
        s = s.concat(" language ");
        System.out.println("s is "+s);
        System.out.println(System.identityHashCode("s is "+s));
        
        System.out.println("****Non Literal String****");
        String s2 = new String("python");
        String s3 = new String("python");
            System.out.println("s2 is "+s2);
            System.out.println("s3 is "+s3);
            System.out.println(System.identityHashCode(s2));//identityHashcode should be in ITALIC FONT
            System.out.println(System.identityHashCode(s3));
        s2 = s2.concat(" language ");    
        	System.out.println("s2 is "+s2);
            System.out.println(System.identityHashCode(s2));//identityHashcode should be in ITALIC FONT

    }
}
/* Run time output
****Literal String****
s is Java
S1 is Java
1067040082
1067040082
s is Java language 
1325547227
****Non Literal String****
s2 is python
s3 is python
980546781
2061475679
s2 is python language 
140435067
*/